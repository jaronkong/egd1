        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Update

for ( var i = 0; i &lt; ds_list_size( meteorList ); i += METEOR.NUM_PROPERTIES ) {
    var tDir = meteorList[| i + METEOR.DIR];
    var tCurrentX = meteorList[| i + METEOR.X];
    var tCurrentY = meteorList[| i + METEOR.Y];
    var tSpeed = meteorList[| i + METEOR.SPEED];
    
    tCurrentX += lengthdir_x( tSpeed, tDir );
    tCurrentY += lengthdir_y( tSpeed, tDir );
    
    tSpeed = lerp( tSpeed, 0, speedTween );
    
    meteorList[| i + METEOR.SPEED] = tSpeed;
    meteorList[| i + METEOR.X] = tCurrentX;
    meteorList[| i + METEOR.Y] = tCurrentY;
}

// Remove old meteors

for ( var i = 0; i &lt; ds_list_size( meteorList ); i += METEOR.NUM_PROPERTIES ) {

    var tSpeed = meteorList[| i + METEOR.SPEED];
    if ( tSpeed &lt; minSpeed ) {
        for ( var j = 0; j &lt; METEOR.NUM_PROPERTIES; ++j ) {
            ds_list_delete( meteorList, i );
        }
    }
}


</string>
          </argument>
        </arguments>
